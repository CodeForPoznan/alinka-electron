{
  "name": "alinka",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Code for Poznan",
    "email": "hello@codeforpoznan.pl",
    "url": "https://codeforpoznan.pl/"
  },
  "description": "Aplication for Supporting Center for Children",
  "main": "main.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeForPoznan/alinka-electron/issues"
  },
  "homepage": "https://github.com/CodeForPoznan/alinka-electron#readme",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "webpack:dev": "MODE=dev webpack-dev-server --mode development --progress",
    "electron:dev": "MODE=dev electron .",
    "start": "concurrently \"cross-env BROWSER=none npm run webpack:dev\" \"wait-on http://localhost:9000 && npm run electron:dev\"",
    "build": "webpack --mode production && electron-builder --windows --linux -p never",
    "release": "webpack --mode production && electron-builder --windows --linux -p always",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "docx": "^4.7.1",
    "file-system": "^2.2.2",
    "final-form": "^4.18.5",
    "final-form-arrays": "^3.0.1",
    "jszip": "3.2.2",
    "nunjucks": "^3.2.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^3.0.11",
    "electron-builder": "^20.38.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-react-devtools": "^0.5.3",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-jest": "23.0.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "prettier": "1.17.1",
    "react-test-renderer": "16.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "wait-on": "^3.2.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1"
  },
  "build": {
    "productName": "Alinka",
    "appId": "com.alinka.app",
    "asar": true,
    "publish": {
      "provider": "s3",
      "bucket": "alinka-dev",
      "path": "installer"
    },
    "win": {
      "target": {
        "target": "NSIS",
        "arch": [
          "x64",
          "ia32"
        ]
      },
      "icon": "./assets/icon.png"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "linux": {
      "target": {
        "target": "deb",
        "arch": [
          "x64",
          "ia32"
        ]
      },
      "icon": "./assets/icon.png"
    },
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "package.json",
      "build/",
      "assets/",
      "src/docx/",
      "src/utils/",
      "main.js",
      "node_modules/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
