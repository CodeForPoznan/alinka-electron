{
  "plugins": ["prettier", "react"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  }
}
